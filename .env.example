PORT=8080
# 设为空（或 "" / null / undefined）会自动关闭鉴权
API_TOKEN=replace-with-strong-token
# 可选：false 时强制关闭鉴权（忽略 API_TOKEN）
AUTH_ENABLED=true
BASE_URL=https://img.example.com
SCAN_INTERVAL_SEC=30
DEDUP_ENABLED=true
DEDUP_WINDOW=20
TOP_K=30
# 每 IP 每秒请求上限，0 表示关闭限流
RATE_LIMIT_RPS=10
UA_TRUST_MODE=auto

# 可选：当请求未传 width/height 时，使用该默认画布返回缩放后的 SVG
DEFAULT_WALLPAPER_WIDTH=0
DEFAULT_WALLPAPER_HEIGHT=0
